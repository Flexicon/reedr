<div id="<%= dom_id feed %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= feed.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Subtitle:</strong>
    <%= feed.subtitle %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Url:</strong>
    <%= feed.url %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= feed.user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Latest:</strong>
    <%= feed.latest_id %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "View", feed, class: "button link" %>
    <%= link_to 'Edit', edit_feed_path(feed), class: "button link ml-2" %>
    <hr class="mt-6">
  <% end %>
</div>
